{
  "name": "backendserver",
  "version": "0.0.1",
  "description": "Proyecto Backend API de GraphQL con TypeScript",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p .",
    "dev": "nodemon \"src/server.ts\" --exec \"ts-node\" \"src/server.ts\" -e ts,graphql,json"
  },
  "keywords": [
    "GraphQL",
    "Cloudinary",
    "MEAN + G",
    "Stripe"
  ],
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.5.1",
    "@graphql-tools/merge": "^8.1.2",
    "@graphql-tools/schema": "^8.2.0",
    "@graphql-tools/utils": "^8.2.5",
    "apollo-server-express": "^3.3.0",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.6.1",
    "graphql-import-node": "0.0.4",
    "graphql-merge-resolvers": "^1.1.16",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-tools": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.3",
    "ncp": "^2.0.0",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.10.3",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1",
    "typescript-tslint-plugin": "^1.0.1"
  }
}
